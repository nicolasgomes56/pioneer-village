{
	"name": "pioneer-village",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"data": {
		"serverVersion": "7290"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "pnpm@9.14.2",
	"scripts": {
		"download-server": "cd artifacts && node download.js",
		"start": "cd ./artifacts/7290 && FXServer.exe +exec ../../server.cfg +set onesync on +set svgui_disable 1 +set sv_lan 1 +sv_enforceGameBuild 1491",
		"watch": "turbo run watch --parallel",
		"build": "turbo run build --parallel",
		"prettier": "turbo run prettier --parallel",
		"migrate": "yarn prisma migrate deploy",
		"ui:dev": "pnpm preact watch --refresh --src ./src/ui/dev --cwd ./resources/ui/",
		"create-resource": "node ./resource-generator.js",
		"prepare": "husky install"
	},
	"dependencies": {
		"@prisma/client": "^6.4.1",
		"@socket.io/redis-adapter": "^8.3.0",
		"@types/object-hash": "^3.0.6",
		"buffer": "^6.0.3",
		"bufferutil": "^4.0.9",
		"colors": "^1.4.0",
		"dotenv": "^16.4.7",
		"howler": "^2.2.4",
		"jsonwebtoken": "^9.0.2",
		"normalize.css": "^8.0.1",
		"object-hash": "^3.0.0",
		"preact": "^10.26.2",
		"prisma": "^6.4.1",
		"prisma-redis-middleware": "^4.8.0",
		"process": "^0.11.10",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"socket.io-redis": "^6.1.1",
		"stream-browserify": "^3.0.0",
		"utf-8-validate": "^6.0.5",
		"ws": "^8.18.1"
	},
	"devDependencies": {
		"7zip": "^0.0.6",
		"@biomejs/biome": "^1.9.4",
		"@citizenfx/client": "2.0.12966-1",
		"@citizenfx/server": "2.0.12966-1",
		"@rspack/cli": "^1.2.5",
		"@rspack/core": "^1.2.5",
		"@types/howler": "^2.2.12",
